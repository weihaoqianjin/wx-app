.out_box{
  position:absolute;
  width:100%;
  height:100%;
  top:0;
}
.mask{
  background: #818181;
  opacity: 0.7;
  width:100%;
  height:100%;
  position: absolute;
  top:0;
  left: 0;
  z-index: 110;
}
.dicSelectBox{
  position: relative;
  margin:0 auto;
  margin-top: 104px; 
  width:290px;
  padding-top: 14px;
  border-radius: 4px;
  background-color: #fff;
  display: flex;
  flex-direction: column;
  align-items:center;
  z-index: 200;
}
.dicSelectBox .title{
  text-align: center;
  padding: 10px 0;
  color:#000;
  border-bottom: 1px solid #e6e6e6;
  width:100%;
}
.dicSelectBox .item{
  position:relative;
  padding: 10px 0;
  width:100%;
  text-align:center;
  font-size: 14px;
  color: #818181;
  border-bottom: 1px solid #e6e6e6; 
}
.dicSelectBox view:last-of-type{
  border: 0;
}
.dicSelectBox .close i{
  position: absolute;
  top:-50px;
  right:20px;
  width: 22px;
  height: 22px;
  border: 2px solid #fff;
  border-radius: 50%;
  background-image: -webkit-linear-gradient(left,#fff,#fff),-webkit-linear-gradient(top,#fff,#fff);
  background-repeat: no-repeat,no-repeat;
  background-position: 3px center, center 3px;
  background-size: 16px 2px, 2px 16px;
  -webkit-transform: rotate(45deg);
  transform: rotate(45deg);
}
.dicSelectBox .close::before{
  position:absolute;
  content:"";
  top:19px;
  right:30px;
  width:6px;
  height:6px;
  border-radius:50%; 
  background-color:#000;
}
.dicSelectBox .close::after{
  position: absolute;
  content: "";
  top: -24px;
  right:32px;
  width: 2px;
  height: 46px;
  background-color: #fff;
}
.dicSelectBox .active{
  color:#45daf8;
}
.dicSelectBox .selected{
  position:absolute;
  right:22px;
  top:12px;
  width:16px;
  height:16px;
}